{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "target": "ES2021",
    "lib": [
      "ES2021"
    ],
    "strict": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "strictNullChecks": true
  },
  "include": [
    "src/**/*.ts"
  ]
}
